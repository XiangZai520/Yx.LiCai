@model YxLiCai.Admin.Models.KendoTextBoxViewModel
<div class="form-group">
    <label class="control-label col-sm-6" for="@Model.Id">@Model.Content</label>
    <div class="col-sm-6 col-md-6">
        <input id="@Model.Id" readonly="@Model.Readonly" value="@Model.Value" class="k-textbox" maxlength="100" />
        <span style="color:red">@Model.Tips</span>
    </div>
</div>
