@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="Globak-x">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport" />
    <meta content="yes" name="apple-mobile-web-app-capable" />
    <meta name="format-detection" content="telephone=no" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <title>交易记录</title>
    <link rel="shortcut icon" href="@YxLiCai.HttpPageHelper.GetRemoteNetURL(YxLiCai.SystemSiteEnum.StaticResourceMallWeb, "favicon.ico")" type="image/x-icon" />
    <link href="@YxLiCai.HttpPageHelper.GetRemoteNetURL(YxLiCai.SystemSiteEnum.StaticResourceMallWeb, "css/m_base.css")" rel="stylesheet">
    <link href="@YxLiCai.HttpPageHelper.GetRemoteNetURL(YxLiCai.SystemSiteEnum.StaticResourceMallWeb, "css/global_x.css")" rel="stylesheet">
    <link href="@YxLiCai.HttpPageHelper.GetRemoteNetURL(YxLiCai.SystemSiteEnum.StaticResourceMallWeb, "css/show.css")" rel="stylesheet">
    <link href="@YxLiCai.HttpPageHelper.GetRemoteNetURL(YxLiCai.SystemSiteEnum.StaticResourceMallWeb, "css/uc/invest_base.css")" rel="stylesheet">
    <script src="@YxLiCai.HttpPageHelper.GetRemoteNetURL(YxLiCai.SystemSiteEnum.StaticResourceMallWeb, "js/loader/sea.js")"></script>
   <script>
       var _hmt = _hmt || [];
       (function () {
           var hm = document.createElement("script");
           hm.src = "//hm.baidu.com/hm.js?4a04f76609debae02c1909eaeb218337";
           var s = document.getElementsByTagName("script")[0];
           s.parentNode.insertBefore(hm, s);
       })();
</script>
      <script src="@YxLiCai.HttpPageHelper.GetRemoteNetURL(YxLiCai.SystemSiteEnum.StaticResourceMallWeb, "js/loader/sea-config.js")"></script>
</head>
<body>

    <div id="gx-touch" class="m-wrap">
        <div class="gx-container">
            <div id="tab-pagination" class="ui-tab swipe-pagination">
                <ul class="ui-tab-nav">
                    <li class="current"><a href="javascript://">月月盈</a></li>
                    <li><a href="javascript://">季季享</a></li>
                    <li><a href="javascript://">年年丰</a></li>
                </ul>
            </div>
            <div id='tab-box' class='swipe'>
                <div class="swipe-wrap">
                    <!-- begin 月月盈 -->
                    <div id="item-month" class="swipe-conts">
                        @Html.Action("Get_User_record",new{ProductType=1})
                    </div>
                    <!-- end 月月盈 -->
                    <!-- begin 季季享 -->
                    <div id="item-quarter" class="swipe-conts">
                        <div class="gxtabs-wrap J_gxtabs">
                            <div class="gxtabs-nav J_gxtabs_nav">
                                <ul>
                                    <li class="active">3个月<b class="triangle-top-sb"></b></li>
                                    <li>6个月<b class="triangle-top-sb"></b></li>
                                </ul>
                            </div>
                            <div class="gxtabs-container J_gxtabs_container">

                                <div class="J_gxtabs_item">
                                    @Html.Action("Get_User_record",new{ProductType=2})
                                </div>
                                <div class="J_gxtabs_item gx-hide">
                                    @Html.Action("Get_User_record",new{ProductType=3})
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- end 季季享 -->
                    <!-- begin 年年丰 -->
                    <div id="item-year" class="swipe-conts">
                        @Html.Action("Get_User_record",new{ProductType=4})
                    </div>
                    <!-- end 年年丰 -->
                </div>
            </div>

        </div>
    </div>
</body>
</html>
<!-- 浮动栏 -->
<div class="yx-fixedBar">
    <a class="yx-back" href="javasrcipt:void(0);">返回</a>
    <a class="yx-home" href="/">首页</a>
</div>
<script>
    seajs.use('User_record');
    seajs.use('fixed_bar');
</script>



